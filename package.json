{
  "name": "homepage",
  "version": "1.0.0",
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "private": true,
  "description": "Ultimate Front-end Template made using NextJs, TailwindCSS, and Typescript",
  "author": {
    "email": "info@einfachalex.net",
    "name": "EinfachAlex",
    "url": "https://einfachalex.net"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EinfachAlex/homepage"
  },
  "homepage": "https://einfachalex.net",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext=jsx,ts,tsx .",
    "lint:fix": "eslint --ext=jsx,ts,tsx . --fix",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install",
    "start:both": "concurrently \"npm run dev\" \"cd GNEAI && npm run start:custom\""
  },
  "dependencies": {
    "@huggingface/agents": "0.0.5",
    "@huggingface/hub": "^0.10.0",
    "@huggingface/inference": "^2.6.4",
    "@jridgewell/sourcemap-codec": "^1.4.15",
    "ai": "2.2.21",
    "axios": "^1.6.2",
    "cli": "^1.0.1",
    "core-js": "2.6.12",
    "debug": "^4.1.1",
    "dotenv": "^16.3.1",
    "eslint-config-prettier": "^8.10.0",
    "fsevents": "^2.3.3",
    "next": "^12.3.4",
    "next-themes": "^0.2.1",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0",
    "sourcemap-codec": "1.4.8",
    "tailwind": "^4.0.0",
    "uuid": "^3.3.2",
    "vercel": "^32.5.5"
  },
  "devDependencies": {
    "@types/node": "^18.18.10",
    "@types/react": "^18.2.37",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.16",
    "buffer": "^6.0.3",
    "concurrently": "^8.2.2",
    "daisyui": "^3.9.4",
    "eslint": "^8.54.0",
    "eslint-config-next": "^12.3.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "postcss": "^8.4.31",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.5",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.{tsx,ts,mdx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [],
  "eslintConfig": {
    "rules": {
      "prettier/prettier": "off",
      "semi": "off"
    }
  }
}
